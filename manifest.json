{
	"manifest_version": 2,
	"name": "Rotten",
	"version": "1.0",

	"permissions": ["activeTab", "tabs", "find"],

	"browser_action": {
		"default_icon": {
			"96": "icons/apple-icon.jpg"
		},
		"default_title": "Rotten",
		"default_popup": "/popup/popup.html"
	},

	"background": {
		"scripts": ["background.js"]
	}
}

